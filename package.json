{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.11.6",
    "apollo-server-lambda": "^2.8.1",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.505.0",
    "axios": "^0.18.1",
    "date-fns": "^1.30.1",
    "emotion": "^9.2.12",
    "graphql": "^14.4.2",
    "lodash": "^4.17.13",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-emotion": "^9.2.12",
    "react-number-format": "^4.0.6",
    "react-sidebar": "^3.0.2",
    "react-transition-group": "^2.5.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0",
    "smoothscroll-polyfill": "^0.4.3",
    "urql": "^1.3.0"
  },
  "scripts": {
    "start": "parcel src/frontend/index.html",
    "build": "parcel build src/frontend/index.html",
    "compile": "tsc",
    "test": "jest",
    "migrate": "ts-node ./src/server/migrate/migrate.ts",
    "build-server": "webpack && cp ./src/server/template.yml ./dist/server",
    "package-server": "aws cloudformation package --template-file ./dist/server/template.yml --output-template-file ./dist/server/serverless-output.yml --s3-bucket reading-data-server",
    "deploy-server": "aws cloudformation deploy --template-file ./dist/server/serverless-output.yml --stack-name ReadingData --capabilities CAPABILITY_IAM",
    "deploy": "yarn build-server && yarn package-server && yarn deploy-server"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.31",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^8.0.47",
    "@types/papaparse": "^4.5.5",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-sidebar": "^3.0.0",
    "@types/uuid": "^3.4.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "mocker-data-generator": "^2.6.6",
    "parcel-bundler": "^1.11.0",
    "react-testing-library": "^5.4.2",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-node": "^8.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "@/([^\\.]*)$": "<rootDir>/src/$1",
      "^.+\\.(jpg|png|css|less)$": "<rootDir>/src/test-utils/empty-module.ts"
    },
    "transform": {
      "\\.(ts|tsx|js)$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      },
      "jsdom": true
    },
    "testRegex": "(/__tests__/.*|\\.*(test))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test-utils/test-setup.js",
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost/"
  }
}
